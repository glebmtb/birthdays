<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN"
    "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='noteBook'>
  <inherits name="ru.n5g.birthdays.core.Core"/>
  <inherits name="ru.n5g.birthdays.components.Components"/>
  <inherits name="ru.n5g.birthdays.note_book.contact.Contact"/>
  <inherits name="ru.n5g.birthdays.note_book.event.Event"/>

  <inherits name='com.google.gwt.user.User'/>
  <inherits name='com.extjs.gxt.ui.GXT'/>
  <inherits name="com.extjs.gxt.desktop.WebDesktop"/>
  <inherits name="com.google.gwt.place.Place"/>
  <inherits name="com.google.gwt.activity.Activity"/>

  <entry-point class='ru.n5g.birthdays.note_book.app_note_book.client.MainEntryPoint'/>

  <replace-with class="ru.n5g.birthdays.note_book.app_note_book.client.factory.AppNoteBookClientFactoryImpl">
    <when-type-is class="ru.n5g.birthdays.note_book.app_note_book.client.factory.AppNoteBookClientFactory"/>
  </replace-with>

  <source path='client'/>
</module>
